<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <!-- CSS only -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="icon"
          href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}

</head>
<body>
<div class="container">
    <header class="d-flex justify-content-center py-3">
        <ul class="nav nav-pills">
            <li class="nav-item"><a href="{{ url('index_index') }}" class="nav-link active" aria-current="page">Home</a>
            </li>
            <li class="nav-item"><a href="{{ url('index_create') }}" class="nav-link">PhotoByTilek</a></li>
            <li class="nav-item"><a href="{{ url('app_login') }}" class="nav-link">Войти</a></li>
            <li class="nav-item"><a href="{{ url('app_logout') }}" class="nav-link">Выйти</a></li>
        </ul>
    </header>

</div>
<hr class="w-100">

<div class="container">
    {% block body %}{% endblock %}
</div>


<ul class="nav col-12 col-md-auto mb-2 justify-content-center mb-md-0">
    {% block home_color %}{% endblock %}
</ul>
</body>
</html>
